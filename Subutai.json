{
  "name": "${envName}",
  "description": "Simple Subutai blueprint",
  "version": "2.0.0",
  "author": "github.com/zubaidullo",
  "containers":[
    {
      "hostname":"subutai-container",
      "template":"debian-stretch",
      "peer-criteria":"free peers",
      "size":"TINY",
      "port-mapping":[
        {
          "protocol":"tcp",
          "domain":"${domainName}",
          "external-port":"80",
          "internal-port":"80"
        }
      ]
    }
  ],
  "peer-criteria":[
    {
      "name": "free peers",
      "max-price": "50"
    }
  ],
  "ansible-configuration": {
    "source-url": "https://github.com/zubaidullo/subutai-blueprints/archive/master.zip",
    "ansible-playbook": "main.yaml",
    "extra-vars": [
      {
        "key": "htmlFileName",
        "value": "about.html"
      },
      {
        "key": "htmlContent",
        "value": "test"
      }
    ],
    "groups": [
      {
        "name": "web-group",
        "hostnames": [ "subutai-container" ]
      }
    ]
  },
  "user-variables":
  {
    "envName": {
      "description": "Enter environment name, must container only letters",
      "type": "string",
      "default": "test",
      "validation": "[a-zA-Z]+"
    },
    "domainName": {
      "description": "Select domain name",
      "type": "domain",
      "default": "",
      "validation": "[a-zA-Z0-9]+"
    }
  }
}
